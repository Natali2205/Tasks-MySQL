CREATE TABLE Competition(
competition_id int,
 competition_name varchar(255),
  world_record int, 
set_date int,
);


CREATE TABLE Result(
competition_id int,
 sportsman_id int, 
 result int, 
 city varchar(255),
 hold_date int,
);

CREATE TABLE Sportsman(
sportsman_id int, 
sportsman_name int,
rank int,
year_of_birth int,
 personal_record int, 
 country varchar(255)
);

//1972	10.07	 Valeriy Borzov (URS)	Munich
