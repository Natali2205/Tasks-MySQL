//2000	9.86	 Maurice Greene (USA)	Berlin
INSERT INTO Competition(competition_id , competition_name , world_record , set_date )
VALUES('01','run', '10.07', 1972),('02','run', '9.9','1974'),('04','run','9.86', '2000');

